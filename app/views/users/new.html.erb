<section class="container">
    <div class="row">
        <div class="col-md-6 col-md-offset-3">
            <h1>Sign Up</h1>
            <%= form_for @user do |p| %>
                <%= p.label :username%><br>
                <%= p.text_field :username%><br>
                <%= p.label :password%><br>
                <%= p.password_field :password%><br><br>
                <%= p.submit %>
            <% end %>
        </div>
    </div>        
</section>
